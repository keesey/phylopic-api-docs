  "description": "Documentation for PhyloPic's Application Programming Interface",
  "devDependencies": {
    "api-spec-converter": "^2.7.31",
    "bootprint": "^1.0.2",
    "bootprint-json-schema": "^2.0.0-rc.3",
    "bootprint-openapi": "^1.1.0",
    "cryptiles": "^4.1.3",
    "customize": "^3.0.0",
    "handlebars": "^4.1.0",
    "hoek": "^6.1.2",
    "jquery": "^3.3.1",
    "s3-deploy": "^1.3.0",
    "swagger-cli": "^2.1.1",
    "yarn": "^1.13.0"
  },
  "homepage": "https://github.com/keesey/phylopic-api-docs#readme",
  "keywords": [
    "api",
    "documentation",
